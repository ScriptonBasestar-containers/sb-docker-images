############
# Database
############

POSTGRES_HOST=db
POSTGRES_DB=postgres
POSTGRES_PORT=5432
# Database port on host machine
DB_HOST_PORT=5432

############
# API Configuration
############

# Your site URL (external)
SITE_URL=http://localhost:3000
API_EXTERNAL_URL=http://localhost:8000

# Internal API URL (for service communication)
KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443

############
# Auth - Email
############

# SMTP settings for email authentication
SMTP_ADMIN_EMAIL=admin@example.com
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_SENDER_NAME=Supabase

# Auth settings
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
MAILER_URLPATHS_CONFIRMATION=/auth/v1/verify
MAILER_URLPATHS_INVITE=/auth/v1/verify
MAILER_URLPATHS_RECOVERY=/auth/v1/verify
MAILER_URLPATHS_EMAIL_CHANGE=/auth/v1/verify

############
# Auth - Phone
############

ENABLE_PHONE_SIGNUP=false
ENABLE_PHONE_AUTOCONFIRM=false

############
# Studio
############

STUDIO_PORT=3000
STUDIO_DEFAULT_ORGANIZATION=Default Organization
STUDIO_DEFAULT_PROJECT=Default Project
SUPABASE_PUBLIC_URL=http://localhost:8000

############
# Storage
############

STORAGE_BACKEND=file
# For S3 storage, set:
# STORAGE_BACKEND=s3
# GLOBAL_S3_BUCKET=your-bucket
# AWS_DEFAULT_REGION=your-region
# AWS_ACCESS_KEY_ID=your-key
# AWS_SECRET_ACCESS_KEY=your-secret

FILE_SIZE_LIMIT=52428800
FILE_STORAGE_BACKEND_PATH=/var/lib/storage

############
# Functions
############

FUNCTIONS_VERIFY_JWT=false

############
# Analytics (Logflare)
############

LOGFLARE_API_KEY=your-logflare-api-key

############
# Pooler (Supavisor)
############

POOLER_ENABLED=true
POOLER_DEFAULT_POOL_SIZE=15
POOLER_MODE=transaction
POOLER_PORT=6543

############
# Image Proxy
############

IMGPROXY_ENABLE_WEBP_DETECTION=true

############
# Additional Settings
############

# Set to 'production' for production deployment
ENVIRONMENT=development

# Timezone
TZ=UTC
